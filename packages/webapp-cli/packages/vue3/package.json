{
  "name": "vue-template",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite --host --open",
    "build": "run-p type-check build-only",
    "build-development": "run-p type-check build-only-development",
    "build-staging": "run-p type-check build-only-staging",
    "preview": "vite preview --host --open",
    "build-only": "vite build",
    "build-only-staging": "vite build --mode staging",
    "build-only-development": "vite build --mode development",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@unocss/reset": "^0.51.8",
    "axios": "^1.3.6",
    "pinia": "^2.0.35",
    "vant": "^4.2.0",
    "vconsole": "^3.15.0",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@types/node": "^18.16.0",
    "@unocss/eslint-config": "^0.51.8",
    "@unocss/transformer-attributify-jsx": "^0.51.8",
    "@unocss/transformer-directives": "^0.51.8",
    "@unocss/transformer-variant-group": "^0.51.8",
    "@vant/eslint-config": "^3.5.0",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.39.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.23",
    "postcss-px-to-viewport-8-plugin": "^1.2.2",
    "prettier": "^2.8.7",
    "typescript": "~4.8.4",
    "unocss": "^0.51.8",
    "unplugin-auto-import": "^0.15.3",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^4.3.1",
    "vue-tsc": "^1.4.4"
  }
}
